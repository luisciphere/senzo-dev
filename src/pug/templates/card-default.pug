.col-sm-4.col-xs-12
  .the-card.clearfix
    h1.card-location Main Office
    .card-buttons.clearfix
      .card-inner-location.pull-left
        span Meeting Rooms 
        i.fa.fa-chevron-down(aria-hidden="true")
      .card-nav.pull-right
        a.button-sm.pull-right(href="#") LIVE 
        a.button-sm.pull-right.nav-stats(href="#") Stats
    include gauge.pug    
    .card-stats-numeric.clearfix
      .total-rooms-container.text-center.pull-left
        .total-rooms-title
          span Total Rooms
        .total-rooms-number
          span 42
        .total-rooms-inuse
          span#rooms-inuse 12
          span  in use   
      .total-desks-container.text-center.pull-right
        .total-desks-title
          span Total Desks
        .total-desks-number
          span 42
        .total-desks-inuse
          span#desks-inuse 12
          span  in use   
    .card-bottom-menu
      .row
        .col-xs-4.text-center
          a.card-settings(href="#")
            img(src="assets/images/card-settings.svg")
        .col-xs-4.text-center
          a.card-export(href="#")
            img(src="assets/images/export.svg")
        .col-xs-4.text-center
          a.card-settings(href="#")
            img(src="assets/images/maintenance.svg")
         
                   
            
           
